import java.util.*;
class Fibonacci
{
	public static void main(String arg[])
	{
		Scanner sc=new Scanner(System.in);
		int a,b,n,t;
		System.out.print("enter the number of terms");
		t=sc.nextInt();
		System.out.println("fibonacci series upto"+ t +"terms");
		a=0;b=1;
		for(int i=0;i<t;i++)
		{
			System.out.print(a+" ");
			n=a+b;
			a=b;
			b=n;
		}
	
	}
}	
